package org.loopingstatement;
//create project ,class, main method
public class NestedForLoopConcept {

	public static void main(String[] args) {
//for loop inside a for loop is nested for loop
//		create a for loop inside a for loop
		for (int i = 1; i <= 3; i++) {
			for (int j = 1; j <= 3; j++) {
				System.out.print(j+" ");
			}
			System.out.println();
		}
	}

}
